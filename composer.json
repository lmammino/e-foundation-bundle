{
    "name": "lmammino/e-foundation-bundle",
    "description": "Symfony integration for lmammino/e-foundation",
    "keywords": ["e-commerce"],
    "homepage": "https://github.com/lmammino/e-foundation-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Mammino",
            "email": "lucianomammino@gmail.com"
        }
    ],
    "require": {
        "php":                          ">=5.4",

        "symfony/framework-bundle":     ">=2.1,<3.0-dev",
        "doctrine/doctrine-bundle":     "~1.4",
        "lmammino/e-foundation":        "dev-master"
    },
    "autoload": {
        "psr-4": {
            "LMammino\\EFoundationBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
